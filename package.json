{
  "name": "ts2wasm",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "type": "module",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "compile": "",
    "cover": "nyc --reporter=html npm run test:unit",
    "pretest": "npm run compile",
    "test": "mocha",
    "test:integration": "mocha test/*.integration.spec.ts",
    "test:unit": "mocha 'test/!(*.integration).spec.ts'",
    "lint": "npx lint-staged",
    "format": "prettier --config .prettierrc.json --write '**/*.ts'",
    "eslint": "eslint --fix '**/*.ts'",
    "setup-hooks": "ts-node-esm scripts/link-hooks.ts",
    "prepare": "npm run setup-hooks",
    "build": "npx tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wasm-micro-runtime/ts2wasm.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "binaryen": "^110.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/minimist": "^1.2.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.5",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@typescript-eslint/utils": "^5.33.0",
    "chai": "^4.3.6",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-autolinkable-stylish": "1.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "minimist": "^1.2.6",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "sinon": "^14.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix '**/*.ts' --max-warnings 0",
      "prettier --config .prettierrc.json --write '**/*.ts'"
    ]
  },
  "bugs": {
    "url": "https://github.com/wasm-micro-runtime/ts2wasm/issues"
  },
  "homepage": "https://github.com/wasm-micro-runtime/ts2wasm#readme"
